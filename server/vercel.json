{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/users/login",
      "dest": "/api/users/login",
      "methods": ["POST"],
      "headers": {
        "Access-Control-Allow-Origin": "https://fpci-blog-app-interface.vercel.app",
        "Access-Control-Allow-Methods": "POST",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"],
      "headers": {
        "Access-Control-Allow-Origin": "https://fpci-blog-app-interface.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, PATCH",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ]
}
